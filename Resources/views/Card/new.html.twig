{% extends 'SlotMachineSlotBundle::layout.html.twig' %}

{% block body %}
<h1>Card creation</h1>

<form action="{{ path('card_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('card') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
